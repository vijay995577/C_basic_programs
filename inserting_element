#include<stdio.h>
int main(){
    printf("Enter Element : ");
    int ele;
    int pos;

    scanf("%d",&ele);
    printf("Enter Position: ");
    scanf("%d",&pos);
    int arr[] = {2,4,2,3,4,5,6};
    arr[pos]= ele;
    for(int i=0;i< sizeof(arr)/sizeof(int);i++){
        printf("%d ",arr[i]);
    }
}

